int main(){
  int a,b,c;
  printf("Enter a b c :");
  scanf("%d%d%d",&a,&b,&c );
  if(!((a >= 100 && b >= 100)  ||  (a >= 100 && c >= 100) ||   (b >= 100 && c >= 100)  )) printf("CONTINUE");
  else printf("STOP");
return 0;
}

